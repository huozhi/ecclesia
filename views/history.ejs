<script type="text/javascript">

function sendReq(obj){
		var tableArr = document.getElementById(objId).firstChild.nextSibling.getElementsByTagName('tr');

		var postJson = {
			"roomName" : tableArr[0].children[1].innerHTML,
			"host": tableArr[1].children[1].innerHTML,
			"date" : tableArr[2].children[1].innerHTML,
	};

	var xmlhttp;
	if (window.XMLHttpRequest)
	{// code for IE7+, Firefox, Chrome, Opera, Safari
	  xmlhttp=new XMLHttpRequest();
	}
	else
	{// code for IE6, IE5
	  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
	}
	xmlhttp.onreadystatechange=function()
	{
	  if (xmlhttp.readyState==4 && xmlhttp.status==200)
	  {
	  	var getJson;
	  	getJson = xmlhttp.responseText;
	  	getJson = JSON.parse(getJson);
	  	$.cookie("ChartList",getJson['ChartList']);
	  	$.cookie("MarkdownList",getJson['MarkdownList']);
	  	$.cookie("SketchList",getJson['SketchList']);
	  	window.location.href = "/history/historyDetail";
	  }
	}
	var postStr = JSON.stringify(postJson);

	xmlhttp.open("post","/history/query-history",true);
	xmlhttp.setRequestHeader('Content-Type','application/json');
	xmlhttp.send(postStr);
}
var t;
var divW=$('.index').width();
var divH=$('.index').height();
var scale=1.01;//缩放倍数
var objId;
function biging(obj){
	objId = obj;
  divW=scale*divW;
  divH=scale*divH;
  $("#"+obj).css('width',divW);
  $("#"+obj).css('height',divH);
  if(divW <= 300){
    t = setTimeout("biging(objId);",1);
  }
  else
    clearTimeout(t);
}
function smalling(obj){
  divW=280;
  divH=170;
  $("#"+obj).css('width',divW);
  $("#"+obj).css('height',divH);
}
