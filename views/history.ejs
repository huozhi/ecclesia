<script type="text/javascript">
$(document).ready(function(){
	$(".index").click(function (objId){
		$(".his-container").fadeOut();
		$(".detail-container").fadeIn();
		sendReq(objId);
		$(".btn-back").fadeIn();

	});
	$(".btn-back").click(function (){
		$(".btn-back").fadeOut();
		$(".detail-container").fadeOut();
		$(".his-container").fadeIn();
	});
});
function sendReq(obj){
	var xmlhttp;
	if (window.XMLHttpRequest)
	{// code for IE7+, Firefox, Chrome, Opera, Safari
	  xmlhttp=new XMLHttpRequest();
	}
	else
	{// code for IE6, IE5
	  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
	}
	xmlhttp.onreadystatechange=function()
	{
	  if (xmlhttp.readyState==4 && xmlhttp.status==200)
	  {
	  	alert("send ok");
	  }
	}
	var postJson = {
		"Meetings" : {
			"roomName" : "room temp",
			"host": "",
			"date" : "2014/01/01",
			"userList":
			[
				"XX",
				"CC",
			]
		}
	};
	var postStr = JSON.stringify(postJson);
	xmlhttp.open("get",""+postStr,true);
	xmlhttp.send(postStr);
}
var t;
var divW=$('.index').width();
var divH=$('.index').height();
var scale=1.01;//缩放倍数
var objId;
function biging(obj){
	objId = obj;
  divW=scale*divW;
  divH=scale*divH;
  $("#"+obj).css('width',divW);
  $("#"+obj).css('height',divH);
  if(divW <= 300){
    t = setTimeout("biging(objId);",1);
  }
  else
    clearTimeout(t);
}
function smalling(obj){
  divW=280;
  divH=170;
  $("#"+obj).css('width',divW);
  $("#"+obj).css('height',divH);
}

</script>

