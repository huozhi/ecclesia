      <div id="local-video" class="container-frame">
          <div id="localVolBar" class="vol-bar" ></div>
      </div>
    </div>
  </div>
  <!-- end left affix bar -->
  <!-- sidebar of remote client videos -->
  <!-- main board and charts blank -->
  <div class="main">
    <div class="board">
      <div id="reveal" class="reveal impress">
        <div class="slides">
          <section data-markdown>
            <script type="text/template">
              ```cpp
              #include <iostream>
              using namespace std;
              int main() { cout << "hello world" << endl; return 0; }
              ```
            </script>
          </section>
          <section data-markdown>
            <script type="text/template">
              ```cpp
              #include <iostream>
              using namespace std;
              int main() { cout << "hello world" << endl; return 0; }
              ```
            </script>
          </section>
        </div>
      </div>
    </div>
    <!-- chart groove -->
    <% include chart-preview.ejs %>
    <!-- end chart groove -->

    <% include impress-preview.ejs %>

  </div>  <!-- end main -->
  <!-- end right affix bar -->
 <!-- </div> end fluid-->

</div><!--end big block-->
</div>

